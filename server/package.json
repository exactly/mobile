{
  "name": "@exactly/server",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "pkgroll --sourcemap --src .",
    "sourcemaps": "sentry-cli sourcemaps inject -o exactly -p mobile dist && sentry-cli sourcemaps upload -o exactly -p mobile dist",
    "db:push": "drizzle-kit push",
    "test": "bash -c 'if [[ -z \"$GITHUB_ACTIONS\" ]]; then pnpm run /test:/; else set -o pipefail && pnpm run --aggregate-output --reporter-hide-prefix /test:/ 2>&1 | perl -pe \"s|(?<![a-zA-Z0-9/\\.@])([a-zA-Z][^\\s:\\\"]*?/[^\\s:\\\"]+)|server/\\1|g\"; fi'",
    "test:ts": "tsc --pretty $([ \"$GITHUB_ACTIONS\" = \"true\" ] && echo false || echo true)",
    "test:vi": "vitest run",
    "dev": "tsx --watch --env-file=.env --require=./instrument.cjs index.ts"
  },
  "bin": "./dist/index.cjs",
  "files": [
    ".well-known",
    "app",
    "dist",
    "instrument.cjs"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "@alchemy/aa-core": "^3.19.0",
    "@exactly/lib": "exactly/lib#a8895cd",
    "@hono/node-server": "^1.14.1",
    "@hono/sentry": "^1.2.1",
    "@hono/valibot-validator": "^0.5.2",
    "@onesignal/node-onesignal": "^5.0.0-alpha-02",
    "@segment/analytics-node": "^2.2.1",
    "@sentry/node": "^9.12.0",
    "@sentry/profiling-node": "^9.12.0",
    "@simplewebauthn/server": "^13.1.1",
    "@types/debug": "^4.1.12",
    "async-mutex": "^0.5.0",
    "debug": "^4.4.0",
    "drizzle-orm": "^0.41.0",
    "graphql": "^16.10.0",
    "hono": "^4.7.6",
    "ioredis": "^5.6.1",
    "libphonenumber-js": "^1.12.6",
    "pg": "^8.14.1",
    "redis-semaphore": "^5.6.2",
    "valibot": "^1.0.0",
    "viem": "^2.26.5"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@exactly/common": "workspace:^0.0.2",
    "@exactly/plugin": "workspace:^0.0.5",
    "@sentry/cli": "^2.43.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.12",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "@wagmi/core": "^2.16.7",
    "drizzle-kit": "^0.30.6",
    "execa": "^9.5.2",
    "ioredis-mock": "^8.9.0",
    "pkgroll": "^2.12.1",
    "prool": "^0.0.24",
    "remove-accents": "^0.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "webauthn-owner-plugin": "exactly/webauthn-owner-plugin#42b67f8"
  }
}
