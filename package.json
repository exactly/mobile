{
  "name": "@exactly/mobile",
  "version": "0.0.2",
  "main": "./src/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prepare": "(git fetch --unshallow || true) && pnpm run --aggregate-output /prepare:/",
    "prepare:git": "git config blame.ignoreRevsFile .git-blame-ignore-revs || true",
    "prepare:expo": "expo customize tsconfig.json",
    "prepare:wagmi": "wagmi generate",
    "prepare:version": "mkdir -p src/generated && echo \"module.exports = /** @type {const} */ ($(git rev-list --count HEAD))\" > src/generated/versionCode.js",
    "prepare:release": "mkdir -p common/generated && echo \"module.exports = /** @type {const} */ ('$(git describe --match '@exactly/mobile@*' | sed 's/@exactly\\/mobile@/v/')')\" > common/generated/release.js",
    "eas-build-on-success": "expo-updates fingerprint:generate --debug --platform android",
    "test": "pnpm run --aggregate-output /test:/",
    "test:spell": "cspell --gitignore '**'",
    "test:deps": "pnpm audit",
    "test:expo": "expo-doctor",
    "test:build": "expo export --platform web",
    "test:ts:expo": "tsc",
    "test:ts:node": "tsc -p tsconfig.node.json",
    "test:changeset": "changeset status --since origin/main",
    "test:eslint": "eslint --max-warnings 0 --color ."
  },
